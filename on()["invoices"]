Indexes:
    "invoice_pkey" PRIMARY KEY, btree (invid)
    "invoice_orgcode_invoiceno_custid_icflag_key" UNIQUE CONSTRAINT, btree (orgcode, invoiceno, custid, icflag)
    "invoice_invoicenoindex" btree (invoiceno)
    "invoice_orgcodeindex" btree (orgcode)
Foreign-key constraints:
    "fk_goid" FOREIGN KEY (goid) REFERENCES godown(goid)
    "invoice_custid_fkey" FOREIGN KEY (custid) REFERENCES customerandsupplier(custid)
    "invoice_orderid_fkey" FOREIGN KEY (orderid) REFERENCES purchaseorder(orderid)
    "invoice_orgcode_fkey" FOREIGN KEY (orgcode) REFERENCES organisation(orgcode) ON DELETE CASCADE
Referenced by:
